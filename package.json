{
  "name": "toolhunt",
  "version": "1.0.50",
  "scripts": {
    "tw:run": "bunx tailwindcss -i ./src/views/styles/globals.css -o ./public/globals.css --watch",
    "dev": "bun run --watch src/index.ts",
    "start": "concurrently \"bun tw:run\" \"bun dev\""
  },
  "dependencies": {
    "@elysiajs/html": "^1.0.2",
    "@elysiajs/static": "^1.0.3",
    "elysia": "latest",
    "ulidx": "^2.3.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "tailwindcss": "^3.4.3",
    "concurrently": "^8.2.2"
  },
  "module": "src/index.js"
}
